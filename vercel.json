{
    "version": 2,
    "functions": {
        "public/*.php": {
            "runtime": "vercel-php@0.5.2"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/public/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}